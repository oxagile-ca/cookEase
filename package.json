{
  "name": "CookEase",
  "main": "expo-router/entry",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "dev": "dotenvx run -f .env.dev -- cross-env EXPO_NO_DOTENV=1 expo start -c",
    "dev:ios": "dotenvx run -f .env.dev -- cross-env EXPO_NO_DOTENV=1 expo start --ios -c",
    "dev:android": "dotenvx run -f .env.dev -- cross-env EXPO_NO_DOTENV=1 expo start --android -c",
    "dev:web": "dotenvx run -f .env.dev -- cross-env EXPO_NO_DOTENV=1 expo start --web -c",
    "dev:doctor": "dotenvx run -f .env.dev -- cross-env EXPO_NO_DOTENV=1 npx expo-doctor",
    "dev:secret:push": "dotenvx run -f .env.dev -- eas secret:push --scope project --env-file .env.dev --force",
    "dev:secret:list": "dotenvx run -f .env.dev -- eas env:list",
    "dev:build:mobile": "npm run dev:secret:push && EXPO_NO_DOTENV=1 dotenv -e .env.dev -- npx eas build --profile development",
    "dev:build:web": "dotenvx run -f .env.dev -- npx expo export --platform web",
    "dev:deploy:web": "npm run dev:build:web && dotenvx run -f .env.dev -- eas deploy --environment development",
    "dev:config:public": "dotenvx run -f .env.dev -- npx expo config --type public",
    "lint": "expo lint",
    "lint:staged": "lint-staged",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "jest --verbose --passWithNoTests",
    "test:watch": "jest --verbose --passWithNoTests --watch",
    "prepare": "husky"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.4",
    "@gorhom/bottom-sheet": "^5",
    "@hookform/resolvers": "^4.0.0",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-picker/picker": "^2.11.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/drawer": "^7.1.1",
    "@react-navigation/native": "^7.0.14",
    "@reduxjs/toolkit": "^2.5.0",
    "@rneui/themed": "^4.0.0-rc.8",
    "@stripe/stripe-react-native": "^0.41.0",
    "@supabase/supabase-js": "^2.48.1",
    "axios": "^1.7.9",
    "date-fns": "^4.1.0",
    "expo": "~52.0.26",
    "expo-application": "~6.0.2",
    "expo-asset": "~11.0.1",
    "expo-auth-session": "~6.0.3",
    "expo-blur": "~14.0.3",
    "expo-constants": "~17.0.3",
    "expo-font": "~13.0.2",
    "expo-haptics": "~14.0.1",
    "expo-image": "~2.0.4",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "~7.0.4",
    "expo-location": "^18.0.6",
    "expo-random": "^14.0.1",
    "expo-router": "~4.0.17",
    "expo-splash-screen": "~0.29.21",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.1",
    "expo-system-ui": "~4.0.7",
    "expo-updates": "~0.26.13",
    "expo-web-browser": "~14.0.2",
    "jest": "~29.7.0",
    "jest-expo": "~52.0.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.54.2",
    "react-native": "0.76.6",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-maps": "^1.20.1",
    "react-native-paper": "^5.13.1",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "react-redux": "^9.2.0",
    "redux-logger": "^3.0.6",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@dotenvx/dotenvx": "^1.33.0",
    "@testing-library/react-native": "^13.0.1",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "@types/redux-logger": "^3.0.13",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.3",
    "lint-staged": "^15.4.1",
    "prettier": "^3.4.2",
    "react-test-renderer": "18.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  }
}
